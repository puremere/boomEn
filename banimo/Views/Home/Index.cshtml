<script>
    $('.value-plus').on('click', function () {
        event.stopPropagation();
        $("#loading").fadeIn()
        let newVal = $(this).parent().children(".value").text();
        let finalVal = parseInt(newVal) + 1;
        // $(this).parent().children(".value").text(finalVal);
        var ID = $(this).attr('id');
        let myid = "value" + ID.replace("addtocarto", "");

        $("." + myid).text(finalVal);

        $.ajax({
            url: '/Home/addtocart',
            dataType: 'html',
            data: {
                id: ID

            },
            success: function (data) {
                var count = parseInt($(".simpleCart_quantityy").text());
                count += 1;
                $(".simpleCart_quantityy").text(count);
                $("#loading").fadeOut()


            },
            error: function () {
                alert("error")
            }


        });
    });
    $('.value-minus').on('click', function () {
        event.stopPropagation();

        var ID = $(this).attr('id');
        let myid = "value" + ID.replace('minus', '');

        let newVal = $(this).parent().children(".value").text();
        if (newVal != "0") {
            $("#loading").fadeIn()
            let finalVal = parseInt(newVal) - 1;
            $("." + myid).text(finalVal);
            $.ajax({
                url: '/Home/minusfromcart',
                dataType: 'html',
                data: {
                    id: ID
                },
                success: function (data) {
                    var count = parseInt($(".simpleCart_quantityy").text());
                    count -= 1;
                    $(".simpleCart_quantityy").text(count);
                    $("#loading").fadeOut()


                },
                error: function () {
                    alert("error")
                }


            });
        }








    });




</script>





