                                            @Html.DropDownList("income1", new SelectList(Model.List.Where(x => x.type == "درآمد"), "ID", "title"), "-- انتخاب درآمد --", new { id = "partnerListForPartner", style = "width:100%; min-height:43px", @class = "form-control" })
                                        </div>
                                    }
                                </div>
                                <div class="autocomplete col-md-3 col-sm-3 col-xs-12">
                                    <label>مقصد تراکنش :</label>
                                    @{


                                        <div class=" " style="width:100%">
                                            @Html.DropDownList("destination1", new SelectList(Model.List.Where(x => x.type == "بانک" || x.type == "صندوق"), "ID", "title"), "-- انتخاب مقصد تراکنش --", new { id = "catListforPartner", style = "width:100%; min-height:43px", @class = "form-control" })
