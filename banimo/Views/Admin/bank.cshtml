                                            <input id="Desc1" class="form-control" style="width:100%; min-height:43px" />
                                        </div>
                                    }
                                </div>

                                <div id="verifyCat" class="btn btn-primary" style="width:50px; margin-top:28px">تایید</div>

                            </div>
                        </form>
                        <script>
                            $("#verifyCat").click(function () {
                                var daramad = $("#income1").val();
                                var destination = $("#destination1").val();
                                var price = $("#price1").val();
                                var desc = $("#Desc1").val();


                                $.ajax({
                                    url: "setCustomTransaction",
                                    dataType: "html",
                                    data: {
                                        fromSource: daramad,
                                        toSource: destination,
                                        price: price,
                                        desc: desc,
                                        typeto: "2",
                                        typefrom: "2",
                                        sourseID: "6"


                                    },
                                    success: function (data) {
                                        window.location.reload();
                                    }
                                })
                            })
